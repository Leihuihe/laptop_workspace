 *  AutoOps.java -> L2-7
 *  
 *  Print out different autoincrement (++) & autodecrement (--) expressions.
 * 
 */

public class AutoOps
{

	public static void main(String[] args)
	{
//		int i = StdIn.readInt();
//
//		StdOut.println(i++);
//		StdOut.println(i);
//		StdOut.println(++i);
//		StdOut.println(i);
//		
//		StdOut.println(i--);
//		StdOut.println(i);
//		StdOut.println(--i);
//		StdOut.println(i);
		
		int sum = 0;

		int num = 0;

		while(num<10)

		{
			sum += num;

			num++;
		}
		StdOut.println(sum);
		
		
		// See L2 handout for details
		

	}

}
